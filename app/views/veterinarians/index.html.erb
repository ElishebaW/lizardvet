<div class= "main">
  <p id="notice"><%= notice %></p>
  <h1>Listing Veterinarians</h1>
  <%= render 'lizards/sidebar' %>
  <table>
    <thead>
      <tr>
        <th>First name</th>
        <th>Last name</th>
        <th>Years in practice</th>
        <th>Vet university</th>
        <th colspan="3">Manage</th>
      </tr>
    </thead>
    <tbody>
      <% @veterinarians.each do |veterinarian| %>
        <tr>
          <td><%= veterinarian.first_name %></td>
          <td><%= veterinarian.last_name %></td>
          <td><%= veterinarian.years_in_practice %></td>
          <td><%= veterinarian.vet_university %></td>
          <td><%= link_to 'Edit', edit_veterinarian_path(veterinarian) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <br>
  <%= link_to 'New Veterinarian', new_veterinarian_path %> | <%= link_to 'Back', lizards_path %>
</div>
